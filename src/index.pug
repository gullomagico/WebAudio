- var fs = require('fs');
include includes/head
body
  include includes/navbar
  header.container.text-center
    p.text-light.
      This is a collection of examples and experiments with the Web Audio API technology.
  .container.text-center
    .row 
      each page in fs.readdirSync("./src/pages")
        .col-12.col-sm-6.my-2
          .card(class="border-light bg-transparent text-light")
            .card-body
              h5 #{page.slice(0, -4)}